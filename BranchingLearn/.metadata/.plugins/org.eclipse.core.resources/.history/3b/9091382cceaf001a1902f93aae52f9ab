package validation;

public class WalktheTalk {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String num = null ;
		
		for (int i = 1; i <=5; i++) {
			if (i>1) {
				
				System.out.println(num+""+i);
				num=num+""+i;
				
			}
			else  {
				System.out.println(num);
			}
			
		}
	}
}
